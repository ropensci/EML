<?xml version="1.0"?>
<!DOCTYPE eml-attribute PUBLIC "-//ecoinformatics.org//eml-attribute-2.0.0beta6//EN" "eml-attribute.dtd">
<eml-attribute>
    <identifier>jscientist.5.2</identifier>

    <attribute>
        <attributeName> lake</attributeName>

        <attributeLabel> lake</attributeLabel>

        <attributeDefinition>The name of the lake (but you could guess that couldn't you:)</attributeDefinition>

        <unit> </unit>

        <dataType> Text</dataType>

        <attributeDomain>
             <textDomain>
                <definition>*</definition>

             </textDomain>

        </attributeDomain>

        <missingValueCode> </missingValueCode>

        <precision> </precision>

    </attribute>

    <attribute>
        <attributeName> site</attributeName>

        <attributeLabel> site</attributeLabel>

        <attributeDefinition>Code indication specific site</attributeDefinition>

        <unit> </unit>

        <dataType> Text</dataType>

        <attributeDomain>
             <enumeratedDomain>
                <code>N1</code>

                <definition>N1</definition>

             </enumeratedDomain>

             <enumeratedDomain>
                <code>N2</code>

                <definition>N2</definition>

             </enumeratedDomain>

             <enumeratedDomain>
                <code>N3</code>

                <definition>N3</definition>

             </enumeratedDomain>

        </attributeDomain>

        <missingValueCode> </missingValueCode>

        <precision> </precision>

    </attribute>

    <attribute>
        <attributeName> sampdate</attributeName>

        <attributeLabel> sampdate</attributeLabel>

        <attributeDefinition>the date of collectoin in DDMMYYYY format</attributeDefinition>

        <unit> </unit>

        <dataType> Text</dataType>

        <attributeDomain>
             <textDomain>
                <definition>*</definition>

             </textDomain>

        </attributeDomain>

        <missingValueCode> </missingValueCode>

        <precision> </precision>

    </attribute>

    <attribute>
        <attributeName> depth</attributeName>

        <attributeLabel> depth</attributeLabel>

        <attributeDefinition>depth where sample was collected</attributeDefinition>

        <unit> meters</unit>

        <dataType> Integers</dataType>

        <attributeDomain>
             <numericDomain>
                <minimum>1.0</minimum>

                <maximum>5.0</maximum>

             </numericDomain>

        </attributeDomain>

        <missingValueCode> </missingValueCode>

        <precision> </precision>

    </attribute>

    <attribute>
        <attributeName> sampvol</attributeName>

        <attributeLabel> sampvol</attributeLabel>

        <attributeDefinition>sample volume</attributeDefinition>

        <unit> cm3</unit>

        <dataType> Floating Point</dataType>

        <attributeDomain>
             <numericDomain>
                <minimum>10.0</minimum>

                <maximum>10.0</maximum>

             </numericDomain>

        </attributeDomain>

        <missingValueCode> </missingValueCode>

        <precision> </precision>

    </attribute>

    <attribute>
        <attributeName> species</attributeName>

        <attributeLabel> species</attributeLabel>

        <attributeDefinition>Species Name</attributeDefinition>

        <unit> </unit>

        <dataType> Text</dataType>

        <attributeDomain>
             <textDomain>
                <definition>*</definition>

             </textDomain>

        </attributeDomain>

        <missingValueCode> </missingValueCode>

        <precision> </precision>

    </attribute>

    <attribute>
        <attributeName> count</attributeName>

        <attributeLabel> count</attributeLabel>

        <attributeDefinition>numer of the indicated species founf in the sample</attributeDefinition>

        <unit> </unit>

        <dataType> Integers</dataType>

        <attributeDomain>
             <numericDomain>
                <minimum>70.0</minimum>

                <maximum>99.0</maximum>

             </numericDomain>

        </attributeDomain>

        <missingValueCode> </missingValueCode>

        <precision> </precision>

    </attribute>

</eml-attribute>